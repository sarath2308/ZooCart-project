    <script>




        document.addEventListener("DOMContentLoaded", function () {
            const productImages = document.querySelectorAll(".product-image"); // Select all images
            const zoomLens = document.querySelectorAll(".zoom-lens"); // Select all zoom lenses
            const zoomContainers = document.querySelectorAll(".zoom-container"); // Select all zoom containers
    
            productImages.forEach((productImage, index) => {
                const lens = zoomLens[index];
                const container = zoomContainers[index];
    
                productImage.addEventListener("mouseenter", function () {
                    lens.style.display = "block"; // Show zoom lens
                    container.style.display = "block"; // Show zoom container
                });
    
                productImage.addEventListener("mouseleave", function () {
                    lens.style.display = "none"; // Hide zoom lens
                    container.style.display = "none"; // Hide zoom container
                });
    
                productImage.addEventListener("mousemove", function (e) {
                    const bounds = productImage.getBoundingClientRect(); // Get image bounds
                    const x = e.clientX - bounds.left; // Mouse X position relative to image
                    const y = e.clientY - bounds.top; // Mouse Y position relative to image
    
                    // Calculate the position for the zoom lens
                    const lensSize = lens.offsetWidth;
                    lens.style.left = `${x - lensSize / 2}px`;
                    lens.style.top = `${y - lensSize / 2}px`;
    
                    // Calculate the position for the zoomed image inside the container
                    const zoomWidth = container.offsetWidth;
                    const zoomHeight = container.offsetHeight;
                    const offsetX = Math.floor(x * 100 / bounds.width); // Percentage X for zoom
                    const offsetY = Math.floor(y * 100 / bounds.height); // Percentage Y for zoom
    
                    // Set the zoomed-in image in the container
                    container.innerHTML = `<div class="zoomed-image" style="background-image: url('${productImage.src}'); background-position: ${offsetX}% ${offsetY}%;"></div>`;
                });
            });
        });
        document.addEventListener("DOMContentLoaded", function () {
  var proQty = $('.pro-qty');
  proQty.prepend('<span class="fa fa-angle-up dec qtybtn"></span>');
  proQty.append('<span class="fa fa-angle-down inc qtybtn"></span>');
  proQty.on('click', '.qtybtn', function () {
      var $button = $(this);
      var $input = $button.parent().find('input');
      var oldValue = parseFloat($input.val());
      var newVal = oldValue;
      
      if ($button.hasClass('inc')) {
          if(oldValue < 5) {
              newVal = oldValue + 1;
          }
      } else {
          if(oldValue > 1) {
              newVal = oldValue - 1;
          }
      }
      $input.val(newVal);
  });

  // For the second quantity selector (.pro-qty-2)
  var proQty2 = $('.pro-qty-2');
  proQty2.prepend('<span class="fa fa-angle-left dec qtybtn"></span>');
  proQty2.append('<span class="fa fa-angle-right inc qtybtn"></span>');
  proQty2.on('click', '.qtybtn', function () {
      var $button = $(this);
      var $input = $button.parent().find('input');
      var oldValue = parseFloat($input.val());
      var newVal = oldValue;
      
      if ($button.hasClass('inc')) {
          if(oldValue < 5) {
              newVal = oldValue + 1;
          }
      } else {
          if(oldValue > 1) {
              newVal = oldValue - 1;
          }
      }
      $input.val(newVal);
  });

});
function addToCart(pid) {
    $.ajax({
        url: `/addToCart`,
        method: 'POST',
        data: JSON.stringify({ pid: pid }), // Ensure proper data format
        contentType: "application/json", // Specify content type
        dataType: "json", // Expect JSON response
        success: function (response) {
            Swal.fire({
                title: response.message,
                icon: 'success',
                toast: true,
                position: 'top-end',
                showConfirmButton: false,
                timer: 2000
            });
        },
        error: function (xhr) {
            let errorMessage = "Something went wrong!";
            if (xhr.responseJSON && xhr.responseJSON.message) {
                errorMessage = xhr.responseJSON.message;
            }
            Swal.fire({
                title: errorMessage,
                icon: 'error',
                toast: true,
                position: 'top-end',
                showConfirmButton: false,
                timer: 2000
            });
        }
    });
}

